{% model "guests.json" %}
<div class="large-12 columns guests" id="guests">
    <div class="row wrapper">
        <div class="large-12 columns inner">
            <h2>Asistentes</h2>
            <ul class="small-block-grid-1 medium-block-grid-2 large-block-grid-3">
                {% for item in model.guests %}
                <li class="block-item">
                {% if(item.twitter) %}
                    <a href="http://www.twitter.com/@{{ item.twitter }}" target="_blank" class="box link">
                        <div class="avatar">
                            <img src="http://twitter.com/api/users/profile_image/{{ item.twitter }}" alt="{{ item.twitter }}">
                        </div>
                        <div class="data">
                            <p>{{ item.name }}</p>
                            <small class="label round">@{{ item.twitter }}</small>
                        </div>
                    </a>
                {% else %}
                    <div class="box">
                        <div class="avatar">
                            <img src="{{ "icon-avatar.png" | asset_url }}" alt="{{ item.name }}">
                        </div>
                        <div class="data">
                            <p>{{ item.name }}</p>
                        </div>
                    </div>
                {% endif %}
                </li>
                {% endfor %}
            </ul>
        </div>
    </div>
</div><!-- end partners  -->

<div class="large-12 columns partners" id="partners">
    <div class="row wrapper">
        <div class="large-12 columns inner">
            <h2>Patrocina</h2>
            <div class="logos top">
                <a href="http://www.zaragoza.es/ciudad/centros/detalle_Centro?id=5105" target="_blank">
                    <img src="{{ "logo-etopia.png" | asset_url }}" alt="Etopia">
                </a>
                <a href="http://www.zaragoza.es/" target="_blank">
                    <img src="{{ "logo-ayuntamiento-zaragoza.png" | asset_url }}" alt="Ayuntamiento de Zaragoza">
                </a>
                <a href="http://www.cervezasambar.com/" target="_blank">
                    <img src="{{ "logo-ambar.png" | asset_url }}" alt="Cerveza Ambar La Zaragozana">
                </a>
            </div>
            <div class="logos bottom">
                <a href="http://www.linkingpaths.com/" target="_blank">
                    <img src="{{ "logo-linking-paths.png" | asset_url }}" alt="Linking Paths">
                </a>
                <a href="http://cuentica.com/" target="_blank">
                    <img src="{{ "logo-cuentica.png" | asset_url }}" alt="Cuentica">
                </a>
                <a href="http://www.uxlearn.com/" target="_blank">
                    <img src="{{ "logo-ux-learn.png" | asset_url }}" alt="UX Learn">
                </a>
                <a href="http://www.semmantica.com/" target="_blank">
                    <img src="{{ "logo-semmantica.png" | asset_url }}" alt="Semmantica Marketing en Internet">
                </a>
            </div>
        </div>
    </div>
</div><!-- end partners -->

<div class="large-12 columns organize" id="organize">
    <div class="row wrapper">
        <div class="large-12 columns inner">
            <h2>Organizado por <a href="http://cachirulovalley.com/" target="_blank">Cachirulo Valley</a></h2>
        </div>
